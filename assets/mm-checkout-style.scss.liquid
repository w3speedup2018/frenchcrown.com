/* ============================================
				POBOX POPUP STYLE 
=============================================*/
*{
  box-sizing: border-box;
}
.fc_body{
  overflow: hidden;
}
.fc_overlay{
  width: 100%;
  height: 100%;
  position: fixed;
  background: black;
  top: 0;
  right: 0;
  opacity: 0.5;
  z-index: 999;
  display: none;
}
.fc_popupBox{
  background-color: #fefefe;
  margin: auto;    
  max-width:1000px;
  width: 100%;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  z-index: 1000;
  display: none;
}
.fc_popupBox .fc_mainHead{
  background-color:#000;
}
.fc_popupBox .fc_mainHead img.logo__image{
	padding: 10px 20px;
}
.fc_popupBox .fc_popup-heading{
  font-size:18px;   
  font-weight:bold;
}  
.fc_popupBox .fc_center{
  text-align: center;
}
.fc_popupBox p{
  margin: 0;
  font-size: 16px;
}
.fc_popupBox .fc_mainContent{
  padding: 0 20px;
  max-height: 500px;
  overflow: visible;
}
.fc_product-contain p{
  margin-bottom: 20px;
}
.fc_popup-heading img{
  max-height: 80px;
}
.fc_popup-heading h3{
  margin-bottom: 20px;
}
.fc_mainContent h3{
	margin: 25px 0;
    font-weight: bold;
    font-size: 22px;
}
.fc_popupBox .fc_popup-footer{
  padding:10px;
  
}
.fc_popup-footer .fc_footer-text{
  margin-bottom: 0;
  color: #0061d0;
  font-size: 22px;
}
.fc_popup-footer .fc_back-btn{
  background-color: #000;
  color: #fff;
  padding: 12px;
  border: 1px;
  border-radius: 5px;
  font-size: 18px;
  font-weight: bold;
}

.fc_popup-footer .complete-order-btn{
	font-size: 25px;
    margin: 10px 0;
}
@media screen and (max-width:1024px){
  .fc_popup-heading img{
    max-height: 60px;
  }
}
@media screen and (max-width:767px){
  .fc_popupBox{
    max-width:90%;
    max-height:90vh;
    overflow: auto;
  }
  .fc_popupBox p{
    font-size: 12px;
  }
  .fc_popup-heading img{
  }
  .fc_popup-footer .fc_back-btn{
    margin-bottom: 10px;
    font-size: 16px;	
  }
}

/* ============================================
				POBOX POPUP STYLE END
=============================================*/


